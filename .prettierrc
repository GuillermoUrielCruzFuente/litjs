{
	"tabWidth": 4,
	"useTabs": true,
	"bracketSameLine": false,
	"htmlWhitespaceSensitivity": "ignore",
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"printWidth": 100,
	"proseWrap": "always",
	"quoteProps": "consistent",
	"semi": true,
	"singleQuote": false,
	"singleAttributePerLine": true,
	"trailingComma": "es5"
}
